<h1>Propose a New Experiment</h1>
<h2>Enter the Proposal Information Below</h2>

<%= form_for @project do |f| %>
  <%= f.label :title %>:
  <%= f.text_field :title %><br>
  <%= f.label :summary %>:
  <%= f.text_field :summary %><br>
  <%= f.label :hypothesis %>:
  <%= f.text_field :hypothesis %><br>
  <%= f.label :time_budget %>:
  <%= f.number_field :time_budget %><br>
  <%= f.label :students %>:<br>
  <% @students.each do |student| %>
  	<%= f.label student.name %>:
  	<%= check_box('students[ids]', student.id) %><br>
  <% end %>

  <%= f.submit %>
<% end %>