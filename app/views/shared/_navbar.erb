<div id="navbar">
  <% if logged_in? %>
    <%= link_to "Projects Page", projects_path, class: 'nav_link' %>
    <% if is_student? 
      user_type = "Student"
    else
      user_type = "Professor" 
    end %>
  <%= link_to "#{user_type} Profile", current_user, class: 'nav_link' %>
  <%= link_to "Logout", logout_path, method: "delete", class: 'nav_link' %>
  <% else %>
    <%= link_to "Register New Student", new_student_path, class: 'nav_link' %>
    <%= link_to "Register New Professor", new_professor_path, class: 'nav_link' %>
    <%= link_to "Login", login_path, class: 'nav_link' %>
  <% end %>
</div>