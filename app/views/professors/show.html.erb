<!-- Prof Prof  -  Professor Profile Page -->

<h1><%=@professor.name%></h1>


<% if @pending_projects.any? %>
<h2>Proposals</h2>
	<ul>
		<% @pending_projects.each do |pending_project| %>
			<li><p><%= link_to "#{pending_project.title}", project_path(pending_project) %></p></li>
		<% end %>
	</ul>
<% end %>

<% if @active_projects.any? %>
	<h2>Active Projects</h2>
	
<% end %>

<% if @completed_projects.any? %>
	<h2>Completed Projects</h2>
	
<% end %>

<% if @rejected_projects.any? %>
	<h2>Rejected Proposals</h2>
	
<% end %>
