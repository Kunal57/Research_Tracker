<h1>Register as a Professor </h1>

<%= form_for @professor, html: {class: "form-group"} do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>
  <%= f.label :email %>
  <%= f.text_field :email %><br>
  <%= f.label :password %>
  <%= f.password_field :password, class: 'form-group password'%><br>
  <%= f.submit %>
<% end %>
